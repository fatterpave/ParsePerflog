<Window x:Class="ParsPerflog.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParsPerflog"
        mc:Ignorable="d"
        Title="MainWindow" Height="200.103" Width="577.766">
    <Grid Margin="0,0,21,20">
        <Button Content="Start" HorizontalAlignment="Left" Margin="23,27,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <RichTextBox x:Name="statusText" HorizontalAlignment="Left" Height="62" Margin="23,63,0,0" VerticalAlignment="Top" Width="502">
            <FlowDocument/>
        </RichTextBox>
        <ProgressBar HorizontalAlignment="Left" Height="16" Margin="97,395,0,0" VerticalAlignment="Top" Width="449"/>
        <ComboBox x:Name="methodCombo" HorizontalAlignment="Left" Margin="171,27,0,0" VerticalAlignment="Top" Width="262" SelectionChanged="methodCombo_SelectionChanged"/>
        <Button x:Name="exportBtn" Content="Export" HorizontalAlignment="Left" Margin="450,27,0,0" VerticalAlignment="Top" Width="75" Click="exportBtn_Click"/>

    </Grid>
</Window>
